:const c_jumpHeight      -15
:const c_maxSpeedLeft    -12
:const c_maxSpeedRight    12
:const c_runAccelerate    3
:const c_airDecelerate    3
:const c_groundDecelerate 6
:const c_gravityStrength  2

:alias a_temp      vd
:alias a_counter   vc


