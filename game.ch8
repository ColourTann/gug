#################################
#
#  Game Loop
#

: main
	hires
	f_setup
	loop
		f_checkScrolling
		f_input
		f_checkCollisionH
		f_gravity
		f_checkCollisionV
		f_delayFPS

		# undraw player before next movement
		#   this would be nicer if it could be easily moved into
		#   the movement logic
		i := player
		load v2
		f_drawAmy
	again
;

: f_setup 
	# draw a bunch of map and setup player starting position
	loop
		f_scroll
		a_counter += 1
	if a_counter != 64 then again
	i := player
	load v1
	a_playerX := 57
	a_playerY := 58
	i := player
	save v2
;

: f_checkScrolling
	push
	i := player
	load a_playerY

	# when player gets too far down, scroll up and draw more row 
	if a_playerY < 30 begin
		f_scroll
	end

	pop
;

: f_input
	push
	i := player
	load v6

	# store the previous x position in case of collision
	a_playerPrevX := a_playerX

	a_temp := 5 # W
	if a_temp key then if a_playerJumpAvailable == 1 begin
		a_playerJumpAvailable := 0
    a_playerSpeedY := c_jumpHeight
	end

	va := c_runAccelerate
: j_input_left
	a_temp := 7 # A
	if a_temp -key then jump j_input_right
	a_playerSpeedX -= va
	if a_playerSpeedX == -15 then a_playerSpeedX := c_maxSpeedLeft
	jump j_input_save

: j_input_right
	a_temp := 9 # D
	if a_temp -key then jump j_input_none
	a_playerSpeedX += va
	if a_playerSpeedX == 15 then a_playerSpeedX := c_maxSpeedRight
	jump j_input_save

: j_input_none
	# if neither left or right are pressed, slow down
	if a_playerSpeedX == 0  then jump j_input_save

	va := c_groundDecelerate
	vb := c_airDecelerate
	if a_playerSpeedX < 128 begin
		if a_playerJumpAvailable == 1 then a_playerSpeedX -= va
		if a_playerJumpAvailable == 0 then a_playerSpeedX -= vb
		if a_playerSpeedX > 128 then a_playerSpeedX := 0
	end
	if a_playerSpeedX > 128 begin
		if a_playerJumpAvailable == 1 then a_playerSpeedX += va
		if a_playerJumpAvailable == 0 then a_playerSpeedX += vb
		if a_playerSpeedX < 128 then a_playerSpeedX := 0
	end

: j_input_save
	i := player
	save v6
	pop
;

# this function does NOT use push/pop because that affects vf and
# drawing is used for collision detection. it instead relies on
# a_playerX and a_playerY being set appropriately by the calling function
: f_drawAmy
	i := amy
	sprite a_playerX a_playerY 3
;

: f_drawAmyPrev
	i := amy
	sprite a_playerPrevX a_playerPrevY 3
;

: f_checkCollisionH
	:alias a_partialDX v7

	push
	i := player
	load v6

	f_drawAmy

	# collide slowly
	#   undraw amy, redraw amy one pixel in the x direction and see if she
	#   collides. if she ever does, revert back one position and exit.
	if a_playerSpeedX < 128 begin
		a_temp :=  1
		a_partialDX >>= a_playerSpeedX
		a_partialDX >>= a_partialDX
	end

	if a_playerSpeedX > 128 begin
		a_temp := -1
		a_partialDX >>= a_playerSpeedX
		a_partialDX >>= a_partialDX
		a_partialDX += 0b11000000
	end

	if a_partialDX == 0 then jump j_checkCollisionH_return

	loop	
		f_drawAmy # undraw
		a_partialDX -= a_temp
		a_playerX += a_temp
		f_drawAmy # redraw
		if vf != 0 begin
			a_playerSpeedX := 0
			f_drawAmy
			a_playerX -= a_temp
			f_drawAmy
			jump j_checkCollisionH_return
		end
	if a_partialDX != 0 then again

: j_checkCollisionH_return
	i := player
	save v6
	pop
;

: f_gravity
	push
	i := player
	load v6

	a_playerSpeedY += c_gravityStrength

	i := player
	save v6
	pop
;

: f_checkCollisionV
	:alias a_partialDY v7

	push
	i := player
	load v6

	# a_temp is the direction the player is traveling

	# a_partialDY is shifted right twice from actual DY.
	# a_partialDY is what's actually used to calculate the new position.
	# this is so playerSpeedY can be increased or decreased more finely
	# might want to do that same with playerY also?

	# < 128 is (probably) positive and > 128 is (probably) negative

	if a_playerSpeedY < 128 begin
		a_temp := 1
		a_partialDY >>= a_playerSpeedY
		a_partialDY >>= a_partialDY
	end 

	if a_playerSpeedY > 128 begin
		a_temp := -1
		a_partialDY >>= a_playerSpeedY
		a_partialDY >>= a_partialDY
		a_partialDY += 0b11000000
	end
	
	if a_partialDY == 0 then jump j_checkCollisionV_return

	# collide slowly-
	# undraw amy, redraw amy one pixel in the y direction and see if she collides
	# if she ever does, revert back one position and exit
	
	loop	
		f_drawAmy
		a_partialDY -= a_temp
		a_playerY += a_temp
		f_drawAmy
		if vf != 0 begin
			if a_temp == 1 then a_playerJumpAvailable := 1
			a_playerSpeedY := 0
			f_drawAmy
			a_playerY -= a_temp
			f_drawAmy
			jump j_checkCollisionV_return
		end
	if a_partialDY != 0 then again

: j_checkCollisionV_return
	i := player
	save v6
	pop
;

: f_delayFPS
	# delay loop to lock framerate and prevent flicker
	loop
		vf := delay
		if vf != 0 then again
	vf := 3
	delay := vf
;

: f_scroll
	# scrolls exactly one row
	# a bit inefficient but I like how it looks when you ascend faster than the scrolling
	push
	i := player
	load a_playerY

	a_playerY += 1
	scroll-down 1
	f_drawMap

	i := player
	save a_playerY
	pop
;

: f_selectMap #TODO- replace with jump table!
	i := long chunk_0
	if a_mapIndex == 1 begin
	  i := long chunk_1
	end
	if a_mapIndex == 2 begin 
	  i := long chunk_2
	end
	if a_mapIndex == 3 begin 
	  i := long chunk_3
	end
;

: f_drawMap
	push
	i := level
	load v3

	# chunkLeftToDraw is an offset to start drawing the chunk data
	# it starts at (map_size-1) so you draw the bottom of the chunk first
	# if it's -1 then you've drawn the top and need to pick a new chunk
	if a_chunkLeftToDraw == -1 begin
		a_mapIndex := random 0b00000011
		f_selectMap
		# the first byte of each chunk is how big that chunk is
		load a_chunkLeftToDraw - a_chunkLeftToDraw
		a_chunkLeftToDraw += -1
	end

	# get to the correct part of the chunk
	f_selectMap
	a_temp := 16
	a_counter := a_chunkLeftToDraw
	loop
		if a_counter == 0 then jump j_drawMap_exit
		i += a_temp
		a_counter += -1
	again

	: j_drawMap_exit

	# plus another 1 because of the size byte
	a_temp := 1
	i += a_temp

	a_bgX := 0
	a_bgY := 0
	a_temp := 1
	a_counter := 0

	loop
		sprite a_bgX a_bgY 1
		i += a_temp
		a_bgX += 8
	if a_bgX != 128 then again
	a_chunkLeftToDraw += -1

	i := level
	save v3	
	pop
;

